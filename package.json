{
  "name": "cy-components",
  "version": "0.1.0",
  "license": "ISC",
  "author": "Kees Kluskens <kees@codeyellow.nl>",
  "description": "React component library for Code Yellow",
  "module": "dist/cy-components.es.js",
  "main": "dist/cy-components.umd.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/CodeYellowBV/cy-components.git"
  },
  "scripts": {
    "start": "start-storybook -p 9001 -c storybook",
    "lint": "eslint src",
    "preversion": "npm run -s lint && npm test && npm run -s build",
    "version": "git add -A dist",
    "build": "rm -f dist/** && node build.js",
    "precommit": "lint-staged",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --tab-width 4 --write",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@storybook/addon-info": "^3.1.6",
    "@storybook/addon-storyshots": "^3.1.8",
    "@storybook/react": "^3.1.8",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^4.1.1",
    "eslint-config-codeyellow": "^4.1.3",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
    "prettier": "^1.5.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "styled-components": "^2.1.0"
  },
  "peerDependencies": {
    "mobx": ">=3.2.1",
    "mobx-react": ">=4.2.2",
    "react": ">=15.0.0",
    "styled-components": ">=1.4.5"
  }
}
